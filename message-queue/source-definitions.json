{
  "rabbit_version": "3.7.8",
  "users": [
    {
      "name": "newsranker",
      "password_hash": "${newsranker.password_hash}",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": ""
    },
    {
      "name": "${admin.user}",
      "password_hash": "${admin.password_hash}",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    },
    {
      "name": "newsscraper",
      "password_hash": "${newsscraper.password_hash}",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": ""
    },
    {
      "name": "streamlistener",
      "password_hash": "${streamlistener.password_hash}",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": ""
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "newsscraper",
      "vhost": "/",
      "configure": "^$",
      "write": "x-news",
      "read": "q-scrape-targets"
    },
    {
      "user": "newsranker",
      "vhost": "/",
      "configure": "^$",
      "write": "x-news",
      "read": "^(q-rank-objects|q-scraped-articles)$"
    },
    {
      "user": "${admin.user}",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "streamlistener",
      "vhost": "/",
      "configure": "^$",
      "write": "x-news",
      "read": "^$"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "rabbit@mq"
    }
  ],
  "policies": [],
  "queues": [
    {
      "name": "q-rank-objects",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "q-scraped-articles",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "q-scrape-targets",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "exchanges": [
    {
      "name": "x-news",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "x-news",
      "vhost": "/",
      "destination": "q-rank-objects",
      "destination_type": "queue",
      "routing_key": "q-rank-objects",
      "arguments": {}
    },
    {
      "source": "x-news",
      "vhost": "/",
      "destination": "q-scrape-targets",
      "destination_type": "queue",
      "routing_key": "q-scrape-targets",
      "arguments": {}
    },
    {
      "source": "x-news",
      "vhost": "/",
      "destination": "q-scraped-articles",
      "destination_type": "queue",
      "routing_key": "q-scraped-articles",
      "arguments": {}
    }
  ]
}
